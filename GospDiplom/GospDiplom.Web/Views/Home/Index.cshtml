@model IEnumerable<GospDiplom.WEB.Models.KioskViewModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nomer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ModelKioska)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Town)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nomer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ModelKioska)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Town)
            </td>
            <td>
                @Html.ActionLink("Order", "MakeOrder", new { KioskId = item.KioskId })
            </td>
        </tr>
    }

</table>