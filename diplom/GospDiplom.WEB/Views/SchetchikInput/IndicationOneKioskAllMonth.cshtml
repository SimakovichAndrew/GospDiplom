@using GospDiplom.BLL.DTO
@model GospDiplom.WEB.Models.SchetchikViewModel


@{
    ViewBag.Title = "IndicationOneKioskAllMonth";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>3 Показания счетчиков киоска№ @ViewBag.Nomer</h2>

<hr />
<table>
    <tr>
        <td>Номер киоска </td>
        <td> Номер счетчика</td>
        <td> Модель счетчика</td>
        <td>Тариф 1 </td>
        <td>Тариф 2 </td>
        <td>Месяц</td>
        <td> Двух тарифный</td>
        <td>Тех учет </td>
        <td> Организация</td>
        <td>Договор </td>
    </tr>
    @foreach (var item in (List<AllCounter>)ViewData["SchetchikAllCounters"])
    {
        <tr>
            <td> @Html.DisplayFor(modelItem => item.NomerKioska)</td>

            <td> @Html.DisplayFor(modelItem => item.NomerCounter)</td>

            <td> @Html.DisplayFor(modelItem => item.ModelCounter)</td>

            <td> @Html.DisplayFor(modelItem => item.Tarif1)</td>

            <td> @Html.DisplayFor(modelItem => item.Tarif2)</td>

            <td> @Html.DisplayFor(modelitem => item.Month) </td>

            <td> @Html.DisplayFor(modelItem => item.TwoTarif)</td>

            <td> @Html.DisplayFor(modelItem => item.TechUchet)</td>

            <td> @Html.DisplayFor(modelItem => item.OrgName)</td>

            <td>@Html.DisplayFor(modelItem => item.Dogovor)</td>
        </tr>
    }
</table>
<hr />